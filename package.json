{
  "name": "clash_royale_deck_builder",
  "version": "1.0.0",
  "description": "Create The Ultimate Clash Royale Deck",
  "main": "index.js",
  "scripts": {
    "go": "node devServer.js",
    "whatStartUsedToBe": "http-server -c-1 build",
    "start": "http-server -c-1 build",
    "dev": "run-p watch serve",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "cpr css build/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daileytj/clash_royale_deck_builder.git"
  },
  "author": "Thomas Dailey",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.0.0",
    "enzyme": "^2.4.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.1",
    "npm-run-all": "^3.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "clashapi": "0.0.2",
    "cors": "^2.8.3",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "http-server": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tools": "^0.13.3",
    "react-transform": "0.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "url-loader": "^0.5.8",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "bugs": {
    "url": "https://github.com/daileytj/clash_royale_deck_builder/issues"
  },
  "homepage": "https://github.com/daileytj/clash_royale_deck_builder#readme"
}
